@model IEnumerable<NetCoreMVC_Otopark.Models.Arac>

@{
    ViewData["Title"] = "TumListe";
}

<h1>Tüm Liste</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AracPlaka)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GirisTarihi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CikisTarihi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Aktiflik)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AracPlaka)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GirisTarihi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CikisTarihi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Aktiflik)
            </td>
            <td>
               
            </td>
        </tr>
}
    </tbody>
</table>
